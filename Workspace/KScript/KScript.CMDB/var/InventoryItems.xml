  <!-- Network configuration -->
  
  <Network>
    <BoundPorts>
      <AppliesToDeviceType>MicrosoftWindows</AppliesToDeviceType>
      <RefreshInterval>30.0:0:0</RefreshInterval>
      <IsMultiValue>TRUE</IsMultiValue>
      <CmdLet>Get-KSNetStat</CmdLet>
      <Parameters>
        <Parameter>
          <Name>ComputerName</Name>
          <Value>%Asset\General\FQDNOrIP%</Value>
        </Parameter>
      </Parameters>
      <OutputPipeline>
        <Expression><![CDATA[ Where-Object { $_.State -in 'LISTENING', $null } ]]></Expression>
      </OutputPipeline>
    </BoundPorts>
    <DNS>
      <ItemName>DNSRecord</ItemName>
      <AppliesToDeviceType>MicrosoftWindows</AppliesToDeviceType>
      <IsMultiValue>TRUE</IsMultiValue>
      <CmdLet>Get-KSDns</CmdLet>
      <Parameters>
        <Parameter>
          <Name>Name</Name>
          <Value>%Asset\General\FQDNOrIP%</Value>
        </Parameter>
        <Parameter>
          <Name>RecordType</Name>
          <Value>A</Value>
        </Parameter>
      </Parameters>
      <OutputPipeline>
        <Expression><![CDATA[ Select-Object -ExpandProperty Answer ]]></Expression>
      </OutputPipeline>
      <Properties>
        <Property>
          <Name>Name</Name>
        </Property>
        <Property>
          <Name>TTL</Name>
        </Property>
        <Property>
          <Name>RecordType</Name>
        </Property>
        <Property>
          <Name>RecordData</Name>
        </Property>
      </Properties>
    </DNS>
    <NetworkInterfaces>
      <AppliesToDeviceType>MicrosoftWindows</AppliesToDeviceType>
      <IsMultiValue>TRUE</IsMultiValue>
      <CmdLet>Get-KSNetworkAdapter</CmdLet>
      <Parameters>
        <Parameter>
          <Name>ComputerName</Name>
          <Value>%Asset\General\FQDNOrIP%</Value>
        </Parameter>
      </Parameters>
    </NetworkInterfaces>
    <RoutingTable>
      <AppliesToDeviceType>MicrosoftWindows</AppliesToDeviceType>
      <IsMultiValue>TRUE</IsMultiValue>
      <CmdLet>Get-KSCimInstance</CmdLet>
      <Parameters>
        <Parameter>
          <Name>ClassName</Name>
          <Value>Win32_IP4RouteTable</Value>
        </Parameter>
        <Parameter>
          <Name>ComputerName</Name>
          <Value>%Asset\General\FQDNOrIP%</Value>
        </Parameter>
        <Parameter>
          <Name>Filter</Name>
          <Value><![CDATA[ Type<>3 ]]></Value>
        </Parameter>
      </Parameters>
      <OutputPipeline>
        <Expression><![CDATA[ Where-Object { ([IPAddress]$_.Destination).AddressFamily -eq 'InterNetwork' } ]]></Expression>
      </OutputPipeline>
      <Properties>
        <Property>
          <Name>Destination</Name>
          <Expression><![CDATA[ [IPAddress]$_.Destination ]]></Expression>
        </Property>
        <Property>
          <Name>Mask</Name>
          <Expression><![CDATA[ [IPAddress]$_.Mask ]]></Expression>
        </Property>
        <Property>
          <Name>Metric</Name>
          <Expression><![CDATA[ $_.Metric1 ]]></Expression>
        </Property>
        <Property>
          <Name>NextHop</Name>
          <Expression><![CDATA[ [IPAddress]$_.NextHop ]]></Expression>
        </Property>
        <Property>
          <Name>InterfaceIndex</Name>
        </Property>
      </Properties>
    </RoutingTable>
  </Network>
 
